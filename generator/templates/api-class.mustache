import '../helpers/http.dart';
{{#imports}}
import '../{{type}}/{{filename}}.dart';
{{/imports}}
class {{className}}{
    {{#methods}}
    
    /** {{{description}}} */
    static Future<{{returnType}}> {{methodName}} (
        HttpClient client,
        {{#parameters}}
        {{{typeName}}} {{name}},
        {{/parameters}}
        {{#requestBody}}
        {{{typeName}}} body
        {{/requestBody}}
    ) {
        Map<String, dynamic> params = new Map();
        {{#queryParameters}}
        params['{{name}}'] = {{name}};
        {{/queryParameters}}
        HttpClientConfig config = HttpClientConfig('{{method}}', "{{{url}}}", params);
        {{#requestBody}}
        {{{bodySerializeFunction}}}
        {{/requestBody}}
        config.bodyContentType = {{{requestBodyContentType}}};
        return client.request(config).then((response){
            if(response.statusCode == 200){
                return {{returnType}}.fromMap(response.mappedBody);
            }
            throw Exception(response.mappedBody);
        });
    }
    {{/methods}}
}